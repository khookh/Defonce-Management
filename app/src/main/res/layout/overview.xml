<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:id="@+id/constraintLayout"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context="controller.fragment.Overview">

    <TextView
            android:text=""
            android:id="@+id/lastd"
            android:gravity="center"
            android:layout_width="349dp"
            android:layout_height="60dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.008"/>
    <ScrollView
            android:layout_width="366dp"
            android:layout_height="196dp"
            android:id="@+id/scrollView"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.488" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lastweek" app:layout_constraintVertical_bias="0.092">
        <LinearLayout android:layout_width="match_parent" android:id="@+id/linear" android:layout_height="wrap_content"
                      android:orientation="vertical"/>
    </ScrollView>
    <TextView
            android:text="Drinks from the last 7 days"
            android:gravity="center"
            android:layout_width="196dp"
            android:layout_height="38dp"
            android:id="@+id/lastweek" android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/lastd" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>